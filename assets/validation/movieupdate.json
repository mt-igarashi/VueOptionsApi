{
  "validation": [
   {
    "field": "title",
    "name": "タイトル",
    "property": "movie",
    "updateflag": "updateflg",
    "rules": [
    {
      "type": "required",
      "message": "COM-0001"
    },
    {
      "type": "length",
      "min": 3,
      "max": 60,
      "message": "COM-0007"
    }]
   },
   {
    "field": "releaseDate",
    "name": "公開日",
    "property": "movie",
    "updateflag": "updateflg",
    "rules": [
    {
      "type": "required",
      "message": "COM-0001"
    }]
   },
   {
    "field": "genre",
    "name": "ジャンル",
    "property": "movie",
    "updateflag": "updateflg",
    "rules": [
    {
      "type": "required",
      "message": "COM-0001"
    },
    {
      "type": "regex",
      "value": "^[A-Z]+[a-zA-Z\"'\\s-]*$",
      "message": "{0}は先頭半角英字大文字 + 半角英字で入力してください。"
    },
    {
      "type": "maxlength",
      "value": 30,
      "message": "COM-0006"
    }]
   },
   {
    "field": "price",
    "name": "価格",
    "property": "movie",
    "updateflag": "updateflg",
    "rules": [
    {
      "type": "required",
      "message": "COM-0001"
    },
    {
      "type": "range",
      "min": 1,
      "max": 100,
      "message": "COM-0008"
    }]
   },
   {
    "field": "rating",
    "name": "評価",
    "property": "movie",
    "updateflag": "updateflg",
    "rules": [
    {
      "type": "required",
      "message": "COM-0001"
    },
    {
      "type": "regex",
      "value": "^[A-Z]+[a-zA-Z0-9\"'\\s-]*$",
      "message": "{0}は先頭半角英字大文字 + 半角英字で入力してください。"
    },
    {
      "type": "maxlength",
      "value": 5,
      "message": "COM-0006"
    }]
   }
  ]
}